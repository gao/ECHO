<script id="tmpl-StudyCreate" type="text/html">
	<div class="StudyCreate">
        <div class="StudyCreate-header heading">
        	<div class="back">
        		<button class="btn btn-inverse btn-mini btnBackHome">Back</button>
        	</div>
        	<div class="title">
        		<span>{{if name}}{{:name}} {{else}}New Study{{/if}}</span>
        	</div>
        </div>
        
        <div class="StudyCreate-content">
        	<p>     
        		<button class="btn btn-primary btn-block btn-large btnDetail">Details</button>
        	</p> 
        	<p>
        		<button class="btn btn-warning btn-block btn-large btnTask" {{if !id}}disabled{{/if}}>Tasks</button>
        	</p> 
        	<p>
        		<button class="btn btn-success btn-block btn-large btnTestUser" {{if !id}}disabled{{/if}}>Users</button>
        	</p> 
        	<p>
        		<button class="btn btn-danger btn-block btn-large btnResult" {{if !id}}disabled{{/if}}>Results</button>
        	</p> 
        </div>
        
        <div class="StudyCreate-footer">
        	<label>Empower Experience</label>
        </div>
	</div>  
</script>